            "label": "classify_clusters",
            "public": true,
            "generated": false,
            "environment": "Shuffle",
            "name": "execute_python",
            "parameters": [
                {
                    "description": "",
                    "id": "",
                    "name": "code",
                    "example": "",
                    "value": "import json, urllib.request, re\n\nresults = self.full_execution.get(\"results\", []) if isinstance(self.full_execution, dict) else []\ncluster_data = {}\nfor r in results:\n    if r.get(\"action\", {}).get(\"label\") == \"query_clusters\":\n        try:\n            p = json.loads(r.get(\"result\", \"{}\"))\n            cluster_data = p.get(\"message\", p) if isinstance(p, dict) else {}\n        except:\n            cluster_data = {}\n\nclusters = cluster_data.get(\"clusters\", [])\nif not clusters:\n    print(json.dumps({\"classifications\": [], \"investigate_items\": [], \"discord_report\": \"No alert clusters found in last 24h.\"}))\nelse:\n    cluster_desc = \"\"\n    for i, c in enumerate(clusters):\n        cluster_desc += f\"\\n{i+1}. {c['rule_description'][:60]} | {c['agent']} | {c['count']} alerts | lvl {c['max_level']}\"\n\n    try:\n        prompt = f'/no_think Classify each cluster. Return JSON: {{\"clusters\": [{{\"id\": 1, \"classification\": \"CAMPAIGN|ROUTINE|INVESTIGATE|MISCONFIG\", \"reason\": \"brief reason\"}}]}}. Clusters:{cluster_desc}'\n        payload = json.dumps({\n            \"model\": \"qwen3:8b\",\n            \"prompt\": prompt,\n            \"system\": \"You are a SOC analyst triaging alert clusters for a home lab SOC. Classify each cluster as CAMPAIGN (expected red team \u2014 only if activity involves VLAN 40 targets 10.10.40.x or known Caldera/sandcat artifacts), ROUTINE (normal lab noise \u2014 Suricata SPAN alerts, AD logon events, systemd restarts, SSH internet brute force on gcp-vm), INVESTIGATE (genuinely needs analyst review), or MISCONFIG (rule tuning needed). Always respond in valid JSON.\",\n            \"stream\": False,\n            \"format\": \"json\",\n            \"keep_alive\": \"24h\",\n            \"options\": {\"num_predict\": 2000, \"temperature\": 0.3}\n        }).encode()\n        req = urllib.request.Request(\"http://10.10.20.30:11434/api/generate\",\n            data=payload, headers={\"Content-Type\": \"application/json\"})\n        resp = urllib.request.urlopen(req, timeout=120)\n        raw = json.loads(resp.read().decode()).get(\"response\", \"\")\n        raw = re.sub(r\"<think>.*?</think>\", \"\", raw, flags=re.DOTALL).strip()\n        llm_result = json.loads(raw) if raw else {}\n    except Exception as e:\n        llm_result = {}\n        raw = f\"LLM error: {str(e)[:60]}\"\n\n    # Parse structured JSON response\n    classifications = []\n    investigate_items = []\n\n    llm_clusters = llm_result.get(\"clusters\", [])\n    if llm_clusters:\n        for lc in llm_clusters:\n            idx = lc.get(\"id\", 0) - 1\n            cat = lc.get(\"classification\", \"ROUTINE\").upper()\n            reason = lc.get(\"reason\", \"\")[:80]\n            if cat not in (\"CAMPAIGN\", \"ROUTINE\", \"INVESTIGATE\", \"MISCONFIG\"):\n                cat = \"ROUTINE\"\n            if 0 <= idx < len(clusters):\n                entry = {**clusters[idx], \"classification\": cat, \"reason\": reason}\n                classifications.append(entry)\n                if cat == \"INVESTIGATE\":\n                    investigate_items.append(entry)\n\n    # Fill any unclassified clusters as ROUTINE\n    classified_ids = {c.get(\"id\", 0) - 1 for c in llm_clusters} if llm_clusters else set()\n    for idx in range(len(clusters)):\n        if idx not in classified_ids:\n            classifications.append({**clusters[idx], \"classification\": \"ROUTINE\", \"reason\": \"unclassified\"})\n\n    # Build Discord report\n    report_lines = [\"**Daily Alert Cluster Triage (last 24h)**\"]\n    cat_counts = {}\n    for c in classifications:\n        cat = c.get(\"classification\", \"?\")\n        cat_counts[cat] = cat_counts.get(cat, 0) + 1\n    report_lines.append(\" | \".join(f\"{k}: {v}\" for k, v in sorted(cat_counts.items())))\n    for c in classifications:\n        emoji = {\"CAMPAIGN\": \"[CAM]\", \"ROUTINE\": \"[RTN]\", \"INVESTIGATE\": \"[INV]\", \"MISCONFIG\": \"[MIS]\"}.get(c.get(\"classification\", \"\"), \"[?]\")\n        report_lines.append(f\"{emoji} {c.get('rule_description','?')[:50]} on {c.get('agent','?')} ({c.get('count',0):,} alerts) - {c.get('reason','')[:60]}\")\n\n    discord_report = \"\\\\n\".join(report_lines)\n    if len(discord_report) > 1900:\n        discord_report = discord_report[:1897] + \"...\"\n\n    print(json.dumps({\n        \"classifications\": classifications,\n        \"investigate_items\": investigate_items,\n        \"discord_report\": discord_report\n    }))",
