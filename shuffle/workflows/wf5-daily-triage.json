            "name": "execute_python",
            "parameters": [
                {
                    "description": "",
                    "id": "",
                    "name": "code",
                    "example": "",
                    "value": "import json, urllib.request, ssl, base64\n\nctx = ssl.create_default_context()\nctx.check_hostname = False\nctx.verify_mode = ssl.CERT_NONE\n\nquery = {\n    \"size\": 0,\n    \"query\": {\"bool\": {\"must\": [\n        {\"range\": {\"timestamp\": {\"gte\": \"now-24h\"}}}\n    ]}},\n    \"aggs\": {\n        \"clusters\": {\n            \"composite\": {\n                \"size\": 20,\n                \"sources\": [\n                    {\"src_ip\": {\"terms\": {\"field\": \"data.srcip\", \"missing_bucket\": True}}},\n                    {\"rule_id\": {\"terms\": {\"field\": \"rule.id\"}}},\n                    {\"agent\": {\"terms\": {\"field\": \"agent.name\"}}}\n                ]\n            },\n            \"aggs\": {\n                \"sample\": {\n                    \"top_hits\": {\n                        \"size\": 1,\n                        \"_source\": [\"rule.description\", \"rule.level\", \"rule.mitre.technique\", \"rule.mitre.id\", \"data.srcip\", \"agent.name\", \"timestamp\"]\n                    }\n                },\n                \"max_level\": {\"max\": {\"field\": \"rule.level\"}}\n            }\n        }\n    }\n}\n\ncreds = \"admin:YOUR_PASSWORD\"\nauth_header = base64.b64encode(creds.encode()).decode()\ntry:\n    req = urllib.request.Request(\n        \"https://10.10.20.30:9200/wazuh-alerts-4.x-*/_search\",\n        data=json.dumps(query).encode(),\n        headers={\"Content-Type\": \"application/json\", \"Authorization\": f\"Basic {auth_header}\"},\n        method=\"POST\")\n    resp = urllib.request.urlopen(req, context=ctx, timeout=15)\n    data = json.loads(resp.read().decode())\n    buckets = data.get(\"aggregations\", {}).get(\"clusters\", {}).get(\"buckets\", [])\n    # Sort by doc_count descending, take top 10\n    buckets.sort(key=lambda x: x.get(\"doc_count\", 0), reverse=True)\n    clusters = []\n    for b in buckets[:10]:\n        key = b.get(\"key\", {})\n        sample_hits = b.get(\"sample\", {}).get(\"hits\", {}).get(\"hits\", [])\n        sample = sample_hits[0].get(\"_source\", {}) if sample_hits else {}\n        rule_desc = sample.get(\"rule\", {}).get(\"description\", \"Unknown\")\n        rule_level = sample.get(\"rule\", {}).get(\"level\", 0)\n        mitre_techs = sample.get(\"rule\", {}).get(\"mitre\", {}).get(\"technique\", [])\n        mitre_ids = sample.get(\"rule\", {}).get(\"mitre\", {}).get(\"id\", [])\n        clusters.append({\n            \"src_ip\": key.get(\"src_ip\", \"none\"),\n            \"rule_id\": key.get(\"rule_id\", \"\"),\n            \"agent\": key.get(\"agent\", \"\"),\n            \"count\": b.get(\"doc_count\", 0),\n            \"max_level\": int(b.get(\"max_level\", {}).get(\"value\", 0)),\n            \"rule_description\": rule_desc,\n            \"mitre_techniques\": mitre_techs[:3] if isinstance(mitre_techs, list) else [],\n            \"mitre_ids\": mitre_ids[:3] if isinstance(mitre_ids, list) else []\n        })\n    print(json.dumps({\"clusters\": clusters, \"total_clusters\": len(buckets)}))\nexcept Exception as e:\n    print(json.dumps({\"error\": str(e)[:200], \"clusters\": []}))",
